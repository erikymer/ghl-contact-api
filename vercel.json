{
  "version": 2,
  "headers": [
    {
      "source": "/embed/insights",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "cron": [
    {
      "path": "/api/refresh-news",
      "schedule": "0 6 * * *"
    }
  ]
}
